{
  "compilerOptions": {
    "lib": [
      "es2021",
      "es2020",
      "es2016.array.include",
      "es2017.object",
      "es5",
      "es6",
      "dom",
      "dom.iterable"
    ],
    "importHelpers": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "outDir": "./client/dist/",
    "module": "commonjs",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es2021",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react",
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
  },
  "includes": [
    "./*"
  ],
  "exclude": [
    "node_modules",
    ".OLD"
  ]
}
